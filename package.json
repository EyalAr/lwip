    "async": "~0.9.0",
    "decree": "0.0.6",
    "nan": "~2.0.9"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "./node_modules/.bin/mocha --opts tests/mocha.opts tests",
