{
  "name": "@randy.tarampi/lwip",
  "description": "Comprehensive, fast, and simple image processing and manipulation",
  "version": "0.0.12",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca",
    "url": "https://www.randytarampi.ca"
  },
  "bugs": {
    "url": "https://github.com/randytarampi/lwip/issues",
    "email": "bugs@randytarampi.ca"
  },
  "dependencies": {
    "async": "^2.6.2",
    "bindings": "^1.5.0",
    "decree": "0.0.6",
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "eslint-plugin-import": "^2.16.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-cpplint": "^0.4.0",
    "nyc": "^13.3.0",
    "run-script-os": "^1.0.5",
    "semantic-release": "^15.13.3",
    "should": "^13.2.3"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "engines": {
    "node": ">=6"
  },
  "gypfile": true,
  "homepage": "https://github.com/randytarampi/lwip",
  "keywords": [
    "alpha",
    "batch",
    "blur",
    "border",
    "buffer",
    "contain",
    "cover",
    "crop",
    "fade",
    "flip",
    "gif",
    "hue",
    "image",
    "jpeg",
    "jpg",
    "lightness",
    "manipulate",
    "mirror",
    "opacity",
    "padding",
    "png",
    "process",
    "resize",
    "rotate",
    "saturation",
    "scale",
    "sharpen",
    "transparency"
  ],
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "randytarampi/lwip.git",
  "scripts": {
    "cover": "npm run cover:actual",
    "cover:actual": "nyc mocha --opts ./tests/mocha.opts tests",
    "cover:collect": "run-script-os",
    "cover:collect:darwin:linux": "cat ./coverage/lcov.info | coveralls",
    "cover:collect:win32": "type ./coverage/lcov.info | coveralls",
    "install": "node-gyp rebuild",
    "pretest": "npm run lint:js",
    "lint": "npm run lint:js && npm run lint:cpp",
    "lint:cpp": "node-cpplint cpp,h src/**/* $(grep ^.*$ CPPLINT.cfg | sed 's/filter=/filters=/g' | awk '{print \"--\"$0}' ORS=' ' | tr '=' ' ' | tr ',' ' ' | tr '/' '-')\n",
    "lint:js": "eslint **/*.js",
    "test": "mocha --opts ./tests/mocha.opts tests",
    "travis": "npm run cover"
  },
  "typings": "typings.d.ts"
}
